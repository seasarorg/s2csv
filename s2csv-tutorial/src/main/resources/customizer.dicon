<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE components PUBLIC "-//SEASAR//DTD S2Container 2.4//EN" 
	"http://www.seasar.org/dtd/components24.dtd">
<components>
  <include path="default-customizer.dicon"/>
  <component name="exampleCustomizer" class="org.seasar.framework.container.customizer.CustomizerChain">
   <initMethod name="addAspectCustomizer">
      <arg>"aop.traceInterceptor"</arg>
      <arg>"(?!^(?:main|getClass|wait|notify|notifyAll)$)(.*)"</arg>
    </initMethod>
  </component>
  
  <component name="csvCustomizer" class="org.seasar.framework.container.customizer.CustomizerChain"/>
    
  <component name="serviceCustomizer" class="org.seasar.framework.container.customizer.CustomizerChain">
    <initMethod name="addAspectCustomizer">
      <arg>"aop.traceInterceptor"</arg>
    </initMethod>
    <initMethod name="addCustomizer">
      <arg>
        <component class="org.seasar.framework.container.customizer.TxAttributeCustomizer"/>
      </arg>
    </initMethod>
  </component>
</components>
